<div class="contents">
  <h3>投稿ページ</h3>
    <%= form_with(model: @prototype, local: true) do |form| %>
      <div class="container">
        <div class="ti"><%= form.text_field :title, placeholder: "Title" %></div>
        <div class="im"><%= form.text_field :image, placeholder: "Image Url" %></div>
        <%= form.submit "投稿する" %>
      </div>
    <% end %>
</div>